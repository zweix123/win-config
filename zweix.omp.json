{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "red",
          "style": "plain",
          "template": "%",
          "type": "root"
        },
        {
          "foreground": "green",
          "properties": {
            "fetch_version": true
          },
          "style": "plain",
          "template": "({{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }}{{ end }}{{ end }})",
          "type": "python"
        },
        {
          "style": "plain",
          "template": "<cyan>{{ .UserName }}</><darkGray>@</><green>{{ .HostName }}</>",
          "type": "session"
        },
        {
          "foreground": "lightYellow",
          "properties": {
            "style": "full"
          },
          "style": "plain",
          "template": "<darkGray>:</>{{ .Path }}",
          "type": "path"
        },
        {
          "style": "plain",
          "template": " <lightBlue>git:(</><lightRed>{{ .HEAD }}</>{{ if .Working.Changed }}<lightYellow>x</>{{ end }}<lightBlue>)</>",
          "type": "git",
          "properties": {
            "fetch_status": true
          }
        },
        {
          "foreground": "red",
          "style": "plain",
          "template": "[{{ if gt .Code 0 }}{{ .Code }}{{ end }}]",
          "type": "exit"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "lightGreen",
          "foreground_templates": [
            "{{ if gt .Code 0}}lightRed{{ end }}"
          ],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": ">",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 3
}